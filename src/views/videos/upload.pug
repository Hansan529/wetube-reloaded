extends base

block content 
  if errorMessage 
    span=errorMessage
  form(method="POST")
    input(name="title" placeholder="제목" type="text" maxlength=50)
    input(name="description" placeholder="설명" type="text" maxlength=140)
    input(name="hashtags" placeholder="태그" type="text")
    input(type="submit" value="Upload Video")